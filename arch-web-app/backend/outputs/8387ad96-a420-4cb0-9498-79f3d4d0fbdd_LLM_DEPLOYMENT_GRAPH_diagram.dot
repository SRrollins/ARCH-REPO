digraph LLMDeploymentGraph {
rankdir=LR;
graph [bgcolor="white", fontname="Helvetica", fontsize=18, labelloc="t", label="LLM Deployment Graph"];
node  [fontname="Helvetica", fontsize=11, style="rounded,filled", penwidth=1.6];
edge  [fontname="Helvetica", fontsize=10, arrowsize=0.9, penwidth=1.4];

subgraph cluster_ingress {
  style="rounded";
  color="#DDDDDD";
  penwidth=1.2;
  label="Ingress / Entry";
  fontname="Helvetica";
  fontsize=12;
  "frontend-lb" [shape="box", fillcolor="#FFCC00", color="#CC9900", style="rounded,filled,solid", label="frontend-lb\n(lb)\nKubernetes Ingress resource acting as external HTTPS load balancer / reverse pro"];
}

